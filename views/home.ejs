<%- include('partials/header') %>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach((post) => { %> 
    <h1><%=post.title%></h1>
    <p><%=post.content.substring(0,100)%>...</p>
    <a href="/posts/<%= post.title.toLowerCase().replace(/ /g, '-') %>">Read more</a>
<% }) %> 

<%- include('partials/footer') %>
